{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#818cf8",
          "image": "./src/assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ]
    ],
    "name": "AdoptaYa",
    "slug": "Adoption-mobile",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.juancamilo.adoptionmobile",
      "edgeToEdgeEnabled": true
    },
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "af7ef88f-cfcc-4ce1-b7bc-0126d2ce881a"
      }
    },
    "owner": "juancam1",
    "ios": {
      "bundleIdentifier": "com.juancamilo.adoptionmobile"
    }
  }
}
